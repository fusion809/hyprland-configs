#!/usr/bin/env zsh
if `which turbo &> /dev/null`; then
	list_unfiltered=$(turbo -Py || sudo pacman -Syup)
        list_filtered=$(echo $list_unfiltered | grep -v "ignored\|core \|extra \|multilib \|testing \|multilib-testing \|extra-testing \|No.*packages\|AUR\|Synchronizing " | grep -vi "pacman" | grep -v "Update:" | grep -v "^$" | grep -v "json" | grep -v "^\s*$")
	no=$(echo $list_filtered | grep -v '^[[:space:]]*$' | wc -l)
fi
if [[ $no -gt 0 ]]; then
	echo "ó°š°"
fi
